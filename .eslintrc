{
    "root": true,
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "quotes": [1, "single"],
        "strict": 0,
        "no-use-before-define": 1,
        "no-underscore-dangle": 0,
        "dot-notation": 1,
        "comma-spacing": 1,
        "no-mixed-requires": 0,
        "new-cap": 1,
        "camelcase": 1,
        "no-redeclare": 0,
        "no-trailing-spaces": 1,
        "no-process-env": 0,
        "no-process-exit": 0,
        "no-unused-vars": [1, { "args": "none" }]
    }
}
